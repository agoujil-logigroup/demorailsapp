<div class="page-header">
  <%= link_to declarations_path, class: 'btn btn-default' do %>
    All Declarations
  <% end %>
  <%= link_to edit_declaration_path(@declaration), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show declaration</h1>
</div>

<dl class="dl-horizontal">
  <dt>User:</dt>
  <dd><%= @declaration.user_id %></dd>

  <dt>Carte:</dt>
  <dd><%= @declaration.carte %></dd>

  <dt>Projet:</dt>
  <dd><%= @declaration.projet %></dd>

  <dt>Produit:</dt>
  <dd><%= @declaration.produit %></dd>

  <dt>Declaration:</dt>
  <dd><%= @declaration.declaration %></dd>

  <dt>Float:</dt>
  <dd><%= @declaration.float %></dd>

</dl>
